numeros = []
print "Digite o primeiro número : "
num1 = gets.chomp.to_i

print "Digite o segundo número : "
num2 = gets.chomp.to_i

print "E por fim digite o terceiro número : "
num3 = gets.chomp.to_i

puts num1 ** 3
puts num2 ** 3
puts num3 ** 3

puts numeros.push("num1","num2", "num3")

"#{num1}","#{num2}", "#{num3}"
